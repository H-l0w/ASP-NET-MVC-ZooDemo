@model ZooDemo.Objects.Pavilon
@using ZooDemo.Objects;
<link rel="stylesheet" href="/css/AddForm.css"/>

<h1>Přidat pavilon</h1>
    <div class="error">
        <h4>@ViewBag.Error</h4>
    </div>
<div class="form_div">
    <div class="form">
        @using (@Html.BeginForm("AddPavilon", "Administration", FormMethod.Post)){
        <label>Jméno pavilonu</label>
        @Html.TextBoxFor(m => m.Name)
        <button type="submit">Přidat</button>
    }
    </div>

</div>